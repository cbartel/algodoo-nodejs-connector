{
  "name": "algodoo-cmd-dispatcher",
  "version": "1.0.0",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts && esbuild ui/main.tsx --bundle --format=esm --platform=browser --outfile=dist/ui/main.js && cp ui/index.html dist/ui/index.html",
    "test": "vitest run"
  },
  "dependencies": {
    "algodoo-server": "workspace:*",
    "ws": "^8.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/ws": "^8.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  }
}
