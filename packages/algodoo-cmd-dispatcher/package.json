{
  "name": "algodoo-cmd-dispatcher",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && tsup ui/main.tsx --format esm --bundle --out-dir dist/ui && cp ui/index.html dist/ui/index.html",
    "test": "vitest run"
  },
  "dependencies": {
    "algodoo-server": "workspace:*",
    "ws": "^8.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/ws": "^8.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  }
}
