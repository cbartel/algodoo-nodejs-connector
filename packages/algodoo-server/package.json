{
  "name": "algodoo-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "module": "dist/server.mjs",
  "types": "dist/server.d.ts",
  "exports": {
    ".": {
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    }
  },
  "bin": {
    "algodoo-server": "dist/index.js"
  },
  "scripts": {
